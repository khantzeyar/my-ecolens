generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CampSite {
  id          Int     @id @default(autoincrement())
  type        String
  name        String
  latitude    Float
  longitude   Float
  state       String
  address     String
  phone       String?
  website     String?
  openingTime String?
  fees        String?
  forestType  String?
  tags        String?
  contact     String?
  imageUrl    String?
  activities  String?   

  @@map("campsite")
}
